{
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "command": {
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true
    },
    "publish": {
      "contents": "dist",
      "message": "chore(release): %s",
      "registry": "http://localhost:4800",
      "createRelease": "github"
    }
  }
}
